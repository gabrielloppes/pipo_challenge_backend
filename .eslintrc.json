{
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": ["standard", "prettier"],
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "globals": {
    "Atomic": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "rules": {
    "no-console": "off",
    "no-unused-vars": "off",
    "no-undef": "off",
    // "prefer-destructuring": "off",
    "max-len": "off"
  }
}
